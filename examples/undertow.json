{
  "config_file": "standalone.xml",
  "domain_mode": false,
  "embedded_mode": true,
  "jboss_home": "C:\\opt\\jboss\\wildfly-10.0.0.Final",
  "server_name": "default-server",
  "host_name": "default-host",
  "facts": false,
  "undertow": {
    "web_filter_deactivate": [
      {
        "name": "proxy-peer",
        "state": "present",
        "class_name": "io.undertow.server.handlers.ProxyPeerAddressHandler",
        "module": "io.undertow.core",
        "priority": 1
      },
      {
        "name": "request-dumper",
        "state": "present",
        "module": "io.undertow.core",
        "class_name": "io.undertow.server.handlers.RequestDumpingHandler",
        "priority": 30
      },
      {
        "name": "notexist",
        "state": "absent"
      }
    ],
    "socket_binding_deactivate": [
      {
        "name": "proxy-https",
        "state": "present",
        "socket_binding_group_name": "standard-sockets",
        "port": 443
      },
      {
        "name": "proxy-https",
        "state": "present",
        "socket_binding_group_name": "standard-sockets",
        "port": 444
      }
    ],
    "http_listener": [
      {
        "name": "proxy-https",
        "state": "present",
        "port": 443,
        "socket_binding": "http",
        "proxy_address_forwarding": true,
        "redirect_socket": "https",
        "enabled": true
      }
    ],
    "ajp_listener": [
      {
        "name": "proxy-https",
        "state": "present",
        "port": 443,
        "socket_binding": "http",
        "redirect_socket": "https",
        "enabled": true
      }
    ]
  }
}
